<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>plan_billableType_cannot_be_null</fullName>
    <active>true</active>
    <description>Do not allow null values for the billableType picklist on Plan</description>
    <errorConditionFormula>ISPICKVAL(billableType__c, &quot;&quot;)</errorConditionFormula>
    <errorDisplayField>billableType__c</errorDisplayField>
    <errorMessage>You must enter a value</errorMessage>
</ValidationRule>
