<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Approving_Managers_Reviewed</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
 ISCHANGED(Status__c),
 Approving_Managers_Reviewed__c = FALSE
)</errorConditionFormula>
    <errorDisplayField>Approving_Managers_Reviewed__c</errorDisplayField>
    <errorMessage>You must be sure that the above managers are correct, and check this box prior to submitting a Compensation Increase Request for approval.</errorMessage>
</ValidationRule>
