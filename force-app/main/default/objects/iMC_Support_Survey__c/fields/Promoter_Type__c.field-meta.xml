<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Promoter_Type__c</fullName>
    <externalId>false</externalId>
    <formula>IF(Recommendation_Probability__c &lt;= 6,&quot;Detractor&quot;,
 IF(Recommendation_Probability__c = 7 || Recommendation_Probability__c = 8,&quot;Passive&quot;,
 IF(Recommendation_Probability__c &gt;= 9,&quot;Promoter&quot;,
&quot;&quot;)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Promoter Type</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
