<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ReadyTalk_Acct_TollFreeNum_required</fullName>
    <active>true</active>
    <description>user is sf admin</description>
    <errorConditionFormula>AND(ISPICKVAL(   Toll_Free_Number__c, &quot;&quot;   ),Provisioned__c, $User.ReadyTalk_Exempt__c &lt;&gt; TRUE,
