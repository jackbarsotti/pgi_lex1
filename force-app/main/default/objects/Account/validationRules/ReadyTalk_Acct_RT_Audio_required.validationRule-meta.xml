<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ReadyTalk_Acct_RT_Audio_required</fullName>
    <active>true</active>
    <description>user is sf admin</description>
    <errorConditionFormula>AND(ISNULL(   Reservationless_Audio__c   ),
Provisioned__c, $User.ReadyTalk_Exempt__c &lt;&gt; TRUE,
RecordType.DeveloperName = &quot;ReadyTalk_Account&quot;)</errorConditionFormula>
    <errorDisplayField>Reservationless_Audio__c</errorDisplayField>
    <errorMessage>Value required to be provisioned</errorMessage>
</ValidationRule>
