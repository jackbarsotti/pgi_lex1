<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ReadyTalk_Acct_Intl_Schedule_Selected</fullName>
    <active>true</active>
    <description>The international schedule can not be none - needed for billing</description>
    <errorConditionFormula>AND(
RecordType.DeveloperName = &quot;ReadyTalk_Account&quot;,
ISPICKVAL( International_Schedule__c , &quot;&quot;))</errorConditionFormula>
    <errorDisplayField>International_Schedule__c</errorDisplayField>
    <errorMessage>An International Schedule must be selected</errorMessage>
</ValidationRule>
