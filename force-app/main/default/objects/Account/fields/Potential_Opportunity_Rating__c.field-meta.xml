<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Potential_Opportunity_Rating__c</fullName>
    <externalId>false</externalId>
    <formula>IF(Adjusted_Annual_Opportunity_Value__c=0,0,
  IF(Adjusted_Annual_Opportunity_Value__c&gt;100000,4,
    IF(Adjusted_Annual_Opportunity_Value__c&gt;50000,3,
      IF(Adjusted_Annual_Opportunity_Value__c&gt;35000,2,
        IF(Adjusted_Annual_Opportunity_Value__c&lt;35001,1,Null)
       )
     )
   )
 )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>4.0 Potential Opportunity Rating

&gt;$100,000 annual opportunity	        4
&gt;$50,000 annual opportunity		3
&gt;$35,000 annual opportunity		2
&lt;$35,000 annual opportunity		1</inlineHelpText>
    <label>Potential Opportunity Rating</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
