<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Utilization__c</fullName>
    <externalId>false</externalId>
    <formula>IF(NumberOfEmployees = 0, 0, Licensed_Internal_Users__c / NumberOfEmployees
  )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Calculation of the licensed users vs the total employees</inlineHelpText>
    <label>Account Utilization</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Percent</type>
</CustomField>
