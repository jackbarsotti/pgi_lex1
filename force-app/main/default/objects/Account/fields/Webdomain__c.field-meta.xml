<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Webdomain__c</fullName>
    <description>Stores the account&#39;s website domain</description>
    <externalId>false</externalId>
    <formula>IF(CONTAINS(Website,&quot;https://www.&quot;),TRIM(SUBSTITUTE(Website,&quot;https://www.&quot;,&quot;&quot;)),
IF(CONTAINS(Website,&quot;HTTPS://WWW.&quot;),TRIM(SUBSTITUTE(Website,&quot;HTTPS://WWW.&quot;,&quot;&quot;)),
IF(CONTAINS(Website,&quot;http://www.&quot;),TRIM(SUBSTITUTE(Website,&quot;http://www.&quot;,&quot;&quot;)),
IF(CONTAINS(Website,&quot;http://&quot;),TRIM(SUBSTITUTE(Website,&quot;http://&quot;,&quot;&quot;)),
IF(CONTAINS(Website,&quot;HTTP://&quot;),TRIM(SUBSTITUTE(Website,&quot;HTTP://&quot;,&quot;&quot;)),
IF(CONTAINS(Website,&quot;https://&quot;),TRIM(SUBSTITUTE(Website,&quot;https://&quot;,&quot;&quot;)),
IF(CONTAINS(Website,&quot;HTTPS://&quot;),TRIM(SUBSTITUTE(Website,&quot;HTTPS://&quot;,&quot;&quot;)),
IF(CONTAINS(Website,&quot;www.&quot;),TRIM(SUBSTITUTE(Website,&quot;www.&quot;,&quot;&quot;)),
IF(CONTAINS(Website,&quot;WWW.&quot;),TRIM(SUBSTITUTE(Website,&quot;WWW.&quot;,&quot;&quot;)),Website)))))))))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Webdomain</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
