<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Round_Robin_ID__c</fullName>
    <description>Used to determine Round Robin for the SMB team, each new user add 1 to the first number, subtract one for any reps leaving the team.</description>
    <externalId>false</externalId>
    <formula>MOD(VALUE(Lead_Number__c) ,5) +1</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>z-Round Robin ID (SMB)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
