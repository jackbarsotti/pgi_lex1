<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_chg_Top10_Mgr_Notes_Unless_Mgr</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
$User.Id &lt;&gt; &#39;0053000000620A0&#39;,
$User.Id &lt;&gt; &#39;005a0000009WIUc&#39;,
$User.Id &lt;&gt; &#39;005300000065o6P&#39;,
$User.Id &lt;&gt; &#39;00530000007dKU2&#39;,
OR(
ISCHANGED(Manager_Only_Notes__c)
)
)</errorConditionFormula>
    <errorMessage>You do not have permission to modify the following fields:  Top 10 Contract Signed, Top 10 Pre Contract, Manager Only Notes.</errorMessage>
</ValidationRule>
