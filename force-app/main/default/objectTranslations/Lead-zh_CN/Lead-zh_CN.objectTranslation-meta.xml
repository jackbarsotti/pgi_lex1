    <validationRules>
        <errorMessage><!-- Lead Name, Company, Phone, Email, and City cannot contain the following special characters: / | \ ; : --></errorMessage>
        <name>ALL_Special_Characters</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- CompanyId should be a numeric value --></errorMessage>
        <name>LID__Check_CompanyId_Value</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Additional details are required when &quot;Other&quot; is selected as Lead Source. --></errorMessage>
        <name>Lead_Source_Other</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- The Type field must be set to a value other than Direct, Field Sales, or Commercial Accounts when the value of Owner&apos;s Channel is Partners. --></errorMessage>
        <name>PGi_AcctType_Cant_Be_Direct_On_Partner</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- You must select an &quot;Account Type&quot; before you&apos;re able to convert this lead. --></errorMessage>
        <name>PGi_Cant_Convert_Without_Account_Type</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- The First Name, Last Name, and/or Company Name values cannot be [not provided] or blank when the Lead Status is set to Qualifying. Please update before saving the record. --></errorMessage>
        <name>PGi_Cant_Qualify_With_Blank_Names</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- The company name cannot be Unknown Trials when the Lead Status is Qualifying.  Please change the Company Name to something else before saving. --></errorMessage>
        <name>PGi_Cant_Qualify_With_Unknown_Trials</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Street, City, State, Postal Code, Country, Phone and Email fields are all required in order to set the Lead Status to Qualifying. --></errorMessage>
        <name>PGi_Cant_Qualify_Without_Address</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- The Department can&apos;t be &quot;Unknown&quot; or blank to save the lead with a Status as Qualifying. --></errorMessage>
        <name>PGi_Cant_Qualify_Without_Department</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- An Initial Product of Interest value is required to save the lead with a Status as Qualifying. --></errorMessage>
        <name>PGi_Cant_Qualify_Without_Product</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Additional details are required when &quot;Other&quot; is selected as Lead Status Reason. --></errorMessage>
        <name>PGi_Disqualified_Details</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- You are not allowed to change the Lead Source once its set on a Lead. --></errorMessage>
        <name>PGi_Enforce_Lead_Source_Value</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- An Opportunity must be created on lead conversion for Leads without a Related Account. Please uncheck the Do not create a new opportunity upon conversion checkbox and press the Convert button again, or update the Related Account Field on the Lead. --></errorMessage>
        <name>PGi_Enforce_Oppty_Creation_on_Conversion</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- An Inbound Color Code is required when &quot;Inbound Call&quot; is selected as the Lead Source. Please complete the &quot;Add Inbound Color Code&quot; field before saving. --></errorMessage>
        <name>PGi_Inbound_Color_Code_Required</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Please select an Industry value that is not Other. --></errorMessage>
        <name>PGi_Industry_Cant_Be_Other_for_APAC</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- All Leads require you to input either a phone number or email address. --></errorMessage>
        <name>PGi_Must_have_Phone_or_Email</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- An Omniture Campaign Code is required for a Lead Source value of &quot;Inbound Call&quot;. --></errorMessage>
        <name>PGi_OmnitureCC_Required_for_Inbound</name>
    </validationRules>
