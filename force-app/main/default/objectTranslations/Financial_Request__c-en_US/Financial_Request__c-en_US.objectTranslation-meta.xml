    <validationRules>
        <errorMessage><!-- Ownership must be a Queue and not a User. Users are to be assigned in the Assigned To area. --></errorMessage>
        <name>All_Owner_Must_Be_a_Queue</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Use 100 characters or less as this field can be used on the customer&apos;s invoice. --></errorMessage>
        <name>Justification_100_character_length</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Billing Cycle is required --></errorMessage>
        <name>PGi_Billing_Cycle_not_required_for_Admin</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- When the Reason for Cancellation is &quot;Changing Providers&quot;, please update the Cancelled Reason Additional Info. --></errorMessage>
        <name>PGi_Cancel_Reason_Changing_Providers</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- When the Reason for Cancellation is &quot;Other&quot;, please update the Cancelled Reason Additional Info. --></errorMessage>
        <name>PGi_Cancel_Reason_Other</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- The Credit Rebill Company Name(s) is required for Credit Rebill requests. --></errorMessage>
        <name>PGi_Credit_Rebill_requires_Rebill_Name</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- You must populate why the ETF does not apply --></errorMessage>
        <name>PGi_ETF_No</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- You must populate who approved the ETF to be waived. --></errorMessage>
        <name>PGi_ETF_Waived</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- You must populate an Invoice Date that the ETF will be applied. --></errorMessage>
        <name>PGi_ETF_Yes</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- In order to set as Approved, the following fields must be populated: PGi Company, Effective Date, Service Plan Type, TCV, # of Licenses, Term, Date of Cancellation, Effective Termination Date, ETF Apply, Contract Renewal Term, and Reason for Cancellation --></errorMessage>
        <name>PGi_Inactivations_Approved</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- The Amount Before Tax cannot be edited after the case is saved. --></errorMessage>
        <name>PGi_No_Edits_to_Amount_Before_Tax</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Once approved, the Estimated Dollar Amount and the Amount (Before Tax) can not be changed. --></errorMessage>
        <name>PGi_No_Edits_to_Approved_NA_Credits</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- The PGi Client cannot be changed once the case is saved. --></errorMessage>
        <name>PGi_No_Edits_to_Client_ID</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Requestor&apos;s Email is required. --></errorMessage>
        <name>PGi_PricingReqtrEmail_notreq_PGiContract</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Sales Rep is required. --></errorMessage>
        <name>PGi_PricingSalesRep_notreq_PGiContract</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Subject line is required. --></errorMessage>
        <name>PGi_PricingSubject_notreq_PGiContract</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Either Related Account or Related Opportunity is required --></errorMessage>
        <name>PGi_Pricing_reqd_OpptyorAcct</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Credit Entry must be a negative number. --></errorMessage>
        <name>RT_Credit_Entry_must_be_Negative</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- When the Duration is marked as &quot;Permanent&quot;, Exemption Removal datee should be blank. --></errorMessage>
        <name>RT_Removal_date_not_required</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- When the Duration is marked as &quot;Temporary&quot;, an Exemption Removal Date is required. --></errorMessage>
        <name>RT_Removal_date_required</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- The Service Date must be within the lat 60 days. --></errorMessage>
        <name>RT_Service_Date_within_60_days</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Anticipated Annual Revenue Loss is required. --></errorMessage>
        <name>RT_SvcFeeExempt_Anticipated_Loss</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Can not create a cancellation request against an inactive client --></errorMessage>
        <name>Require_Active_Client</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Can not create a cancellation request against an inactive Company --></errorMessage>
        <name>Require_Active_Company</name>
