    <rules>
        <fullName>BBS Fire Outbound</fullName>
        <actions>
            <name>BBS_Access_Code_Outbound</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <description>ReadyTalk - BBS Fire outbound access code message on insert/update</description>
