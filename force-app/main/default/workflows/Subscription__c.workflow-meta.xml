            <name>RT_Email_sent_when_Access_Code_Changed_from_Webinar_150_500_3000</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>Sends email to Events team when access code is changed from Webinar 150/500/3000.</description>
        <formula>AND( ISCHANGED(Web_Sub_Default_Type__c), OR(PRIORVALUE(Web_Sub_Default_Type__c) = &quot;Webinar (150)&quot;, PRIORVALUE(Web_Sub_Default_Type__c) = &quot;Webinar (500)&quot;, PRIORVALUE(Web_Sub_Default_Type__c) = &quot;Webinar (3000)&quot;, OR(ISPICKVAL(Web_Sub_Default_Type__c, &quot;None (Per-Minute Rates Apply)&quot;),ISPICKVAL(Web_Sub_Default_Type__c, &quot;Web Meeting&quot;),ISPICKVAL(Web_Sub_Default_Type__c, &quot;Unlimited Webinar&quot;),ISPICKVAL(Web_Sub_Default_Type__c, &quot;Web Meeting Pro (25)&quot;),ISPICKVAL(Web_Sub_Default_Type__c, &quot;Web Meeting Pro (150)&quot;)) ))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
