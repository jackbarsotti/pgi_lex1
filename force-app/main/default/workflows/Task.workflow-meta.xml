    <rules>
        <fullName>RT_Update Type%3A Outbound Call</fullName>
        <actions>
            <name>RT_Update_Type_Outbound_Call</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>InGenius Workflow</description>
        <formula>AND( ispickval(CallType, &quot;Outbound&quot;)&amp;&amp;not(ispickval(Type ,&quot;Meeting&quot;))&amp;&amp;not(ispickval(Type ,&quot;Demo&quot;))&amp;&amp; RecordType.DeveloperName= &#39;ReadyTalk Task&#39;)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Update Type to Note</fullName>
        <actions>
            <name>Update_Type_to_Note</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Task.Subject</field>
            <operation>startsWith</operation>
            <value>Note</value>
        </criteriaItems>
        <description>When subject begins with Note, update activity type to Note.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>iMC - Task%2C Subject Update</fullName>
        <actions>
            <name>iMC_Task_Subject_Update</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Updates the &quot;Subject&quot; with the &quot;Discovery Call&quot; with the &quot;Type&quot; filed is Discovery Call.</description>
        <formula>AND(  RecordType.DeveloperName = &#39;iMC Task&#39;,  ISPICKVAL( Type , &quot;Discovery Call&quot;)&amp;&amp;  LEFT( Subject , 15) &lt;&gt; &quot;Discovery Call:&quot;  )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
</Workflow>
