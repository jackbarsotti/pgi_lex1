    <fieldUpdates>
        <fullName>RFC_Set_CAB_Date</fullName>
        <field>CAB_Review_Date__c</field>
        <formula>TODAY() +
/* before Wednesday */
                IF(Weekday(Today()) &lt; 4, 4- Weekday(TODAY()),
/* is Wednesday */
                IF(Weekday(Today()) == 4, 7,
/* after Wednesday */
                IF(Weekday(Today()) == 5, 6, 
																IF(Weekday(Today()) == 6, 5,		
																IF(Weekday(Today()) == 7, 4, 0
																		)))))</formula>
        <name>RFC-Set CAB Date</name>
