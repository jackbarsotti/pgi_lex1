    </rules>
    <rules>
        <fullName>RFC - Notify Assigned To</fullName>
        <actions>
            <name>RFC_Notify_Assigned_To</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>AND( ISCHANGED( Assigned_To__c ), $User.Id &lt;&gt; Assigned_To__c )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>RFC - Notify Manger of Approval Submission</fullName>
        <actions>
            <name>RFC_Notify_Manger_of_Approval_Submission</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Request_for_Change__c.Status__c</field>
            <operation>equals</operation>
            <value>Pending Approval(s)</value>
        </criteriaItems>
        <criteriaItems>
            <field>Request_for_Change__c.Assigned_To__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <description>Send notification to the Manager of the Assigned To when the request has been submitted for approval</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>RFC - Notify Ownership Group</fullName>
        <actions>
            <name>RFC_Notify_Ownership_Group</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>Notify the Group who owns the RFC when it is created or assigned to them</description>
        <formula>AND(  ISCHANGED( OwnerId ),  ISBLANK (Assigned_To__c) )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
