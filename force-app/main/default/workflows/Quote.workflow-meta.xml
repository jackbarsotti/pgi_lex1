    <fieldUpdates>
        <fullName>iMC_Quote_Expiration_Date</fullName>
        <field>ExpirationDate</field>
        <formula>IF(
CASE(MOD(DATEVALUE (CreatedDate +30) - DATE(1900, 1, 7), 7),
0, 0,
6, 0,
1) &lt;&gt;0, CreatedDate +30,
IF( 
CASE(MOD(DATEVALUE (CreatedDate +31) - DATE(1900, 1, 7), 7),
0, 0,
6, 0,
1) &lt;&gt; 0, CreatedDate +31,
IF(
CASE(MOD(DATEVALUE (CreatedDate +32) - DATE(1900, 1, 7), 7),
0, 0,
6, 0,
1) &lt;&gt; 0, CreatedDate +32, CreatedDate +30
)
)
)</formula>
        <name>iMC - Quote, Expiration Date</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
    </fieldUpdates>
